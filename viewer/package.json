    "@types/graphql": "14.0.1",
    "graphql": "14.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.9",
    "@types/node": "^8.0.31",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/react-router": "^4.0.19",
    "@types/react-router-dom": "^4.2.3",
    "@types/uuid": "^3.4.4",
    "apollo": "^2.1.6",
    "babel-polyfill": "^6.22.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-transform-graphql": "^2.1.0",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.4",
    "redux-mock-store": "^1.5.3"
  },
  "scripts": {
    "build": "yarn run schema && node scripts/build.js",
    "deploy:staging": "yarn run build && firebase use pi-base-staging && firebase deploy",
    "deploy:production": "yarn run build && firebase use pi-base-production && firebase deploy && firebase use pi-base-staging",
    "schema": "cp -r ../graph/* src/graph && apollo client:codegen --localSchemaFile src/graph/schema.gql --target typescript --outputFlat src/types/graph",
    "push-schema": "apollo client:push --clientName=pi-base",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "watch": "node scripts/watchSchema.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>test/setup.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.pegjs$": "<rootDir>/config/jest/pegTransform.js",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "shared"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}