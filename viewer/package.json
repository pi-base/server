{
  "name": "viewer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://viewer.counterexamples.info",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-error": "^1.0.7",
    "apollo-link-http": "1.3.0",
    "autoprefixer": "7.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "fuse.js": "^2.7.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.2",
    "jest": "20.0.4",
    "memoize-one": "^4.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "query-string": "5",
    "react": "^16.4.1",
    "react-apollo": "^2.0.1",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-form": "^7.2.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.3.0",
    "rehype-react": "^3.0.3",
    "remark": "^9.0.0",
    "remark-rehype": "^3.0.1",
    "reselect": "^3.0.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.2.1",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "2.9.2",
    "unified": "^7.0.0",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "@types/jquery": "^3.3.1",
    "@types/node": "^8.0.31",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/react-router": "^4.0.19",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux-form": "^7.3.0",
    "@types/redux-thunk": "^2.1.0",
    "apollo-codegen": "^0.18.1",
    "babel-polyfill": "^6.22.0",
    "graphiql": "^0.11.11",
    "graphql-cli": "^2.0.9",
    "graphql-config": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-transform-graphql": "^2.1.0"
  },
  "scripts": {
    "build": "yarn run schema && node scripts/build.js",
    "deploy": "yarn run build && aws --profile pibase s3 sync build s3://pi-base/deploy/viewer",
    "schema": "apollo-codegen introspect-schema src/graph/schema.gql --output schema.json && apollo-codegen generate src/graph/**/*.gql --schema schema.json --target ts --output src/graph/types.ts",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "watch": "node scripts/watchSchema.js"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
